DOCKER_INFLUXDB_INIT_USERNAME='influx'                 # Initial WebUI username
DOCKER_INFLUXDB_INIT_PASSWORD='YourRandomPasswordHere' # Initial WebUI password
DOCKER_INFLUXDB_INIT_ORG='YourOrgNameHere'             # Initial organization name
DOCKER_INFLUXDB_INIT_BUCKET='YourBucketNameHere'       # Initial bucket, may but doesn't need to be the same as one used in agent
DOCKER_INFLUXDB_INIT_MODE='setup'                      # Change this one only if you don't want initial docker setup
AGENT_INFLUX_URL='http://influxdb:8086'                # scheme://ip:port to influxdb instance, eg.: http://192.168.0.2:8086
AGENT_INFLUX_BUCKET=${DOCKER_INFLUXDB_INIT_BUCKET}     # Influxdb's bucket name; defaults to one created above, but can be changed
AGENT_INFLUX_ORG=${DOCKER_INFLUXDB_INIT_ORG}           # Influxdb's organization; defaults to one created above, but can be changed
AGENT_INFLUX_TOKEN=''                                   # Influxdb token; Needs to be generated from influxdb admin ui
AGENT_VERBOSITY='CRITICAL'                             # Can be one of 'CRITICAL', 'ERROR', 'WARNING', 'INFO', 'DEBUG'
AGENT_HOSTNAME='                                       # Hostname used as tag, if empty defaults to system's hostname
